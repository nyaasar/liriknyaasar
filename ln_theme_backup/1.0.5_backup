}
#ignielRelated h3.title span{
    background-color: var(--htm800);
    padding: 0 15px;
    position: relative;
    z-index: 3;
    border: 2px solid var(--pth200);
}
#ignielRelated h3.title:before{
	content: &#39;&#39;;
