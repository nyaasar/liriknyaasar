    max-width: 720px;
    margin: 20px auto 10px;
    width: calc(80vw - 40px) !important;
}
#ignielRelated h3.title{
  	font-size: calc(1vmin + 12px);
  	font-weight:600;
  	text-align:center;
  	text-transform:capitalize;
